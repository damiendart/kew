declare(strict_types=1);

use Kew\ExampleQueueable;
use Kew\Queue;
use Kew\SystemClock;

require dirname(__DIR__) . '/vendor/autoload.php';

$queue = new Queue(
    __DIR__ . '/queue.db',
    new SystemClock(),
);

$queue->addJob(new ExampleQueueable());
